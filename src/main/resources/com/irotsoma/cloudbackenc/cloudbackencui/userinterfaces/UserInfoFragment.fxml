<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="120.0" prefWidth="282.0">
         <children>
            <TextField fx:id="userInfoUsernameField" layoutY="2.0" prefHeight="25.0" prefWidth="200.0" promptText="%cloudbackencui.label.username" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0" />
            <Button fx:id="userInfoCancelButton" cancelButton="true" layoutX="84.0" layoutY="44.0" mnemonicParsing="false" text="%cloudbackencui.button.cancel" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0" />
            <Button fx:id="userInfoOkButton" defaultButton="true" layoutX="14.0" layoutY="44.0" mnemonicParsing="false" text="%cloudbackencui.button.ok" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" />
            <PasswordField fx:id="userInfoPasswordField" layoutX="14.0" layoutY="47.0" prefHeight="26.0" prefWidth="254.0" promptText="%cloudbackencui.label.password" />
         </children>
      </AnchorPane>
   </children>
</VBox>
