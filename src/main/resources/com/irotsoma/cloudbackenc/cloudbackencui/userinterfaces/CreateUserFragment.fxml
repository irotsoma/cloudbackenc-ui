<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="347.0" prefWidth="200.0">
         <children>
            <TextField fx:id="createUserUsernameField" layoutX="15.0" layoutY="23.0" promptText="%cloudbackencui.label.username" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="20.0" />
            <PasswordField fx:id="createUserPasswordField" layoutX="15.0" layoutY="56.0" promptText="%cloudbackencui.label.password" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="55.0" />
            <PasswordField fx:id="createUserConfirmPasswordField" layoutX="15.0" layoutY="87.0" promptText="%cloudbackencui.label.password.confirm" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="90.0" />
            <TextField fx:id="createUserEmailField" layoutX="14.0" layoutY="125.0" promptText="%cloudbackencui.label.email" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="125.0" />
            <ListView fx:id="createUserRoleList" layoutX="14.0" layoutY="161.0" prefHeight="76.0" prefWidth="172.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="160.0" />
            <Label fx:id="createUserErrorLabel" layoutX="14.0" layoutY="246.0" textFill="#f50101" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="246.0" />
            <Button fx:id="createUserOkButton" defaultButton="true" layoutX="14.0" layoutY="160.0" mnemonicParsing="false" text="%cloudbackencui.button.ok" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" />
            <Button fx:id="createUserCancelButton" cancelButton="true" layoutX="124.0" layoutY="160.0" mnemonicParsing="false" text="%cloudbackencui.button.cancel" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
